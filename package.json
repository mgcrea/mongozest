{
  "name": "mongozest",
  "version": "0.2.3",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "prettycheck": "prettier --check 'packages/*/{src,test}/*.{ts,tsx}'",
    "lint": "eslint --ext .ts,.tsx packages/*/src",
    "clean": "rm -rf node_modules; scripts/runPackages.sh --eval \"rm -rf node_modules\"",
    "build": "NODE_ENV=production scripts/runPackages.sh \"yarn build\"",
    "publish": "scripts/runPackages.sh \"yarn publish --non-interactive --access public\"",
    "website": "cd website; GIT_USER=mgcrea CURRENT_BRANCH=master USE_SSH=true yarn run publish-gh-pages; cd -",
    "version": "scripts/setVersion.sh"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@babel/runtime": "^7.10.2",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-plugin-module-name-mapper": "^1.2.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.1",
    "lodash": "^4.17.15",
    "metro-react-native-babel-preset": "^0.59.0",
    "prettier": "^2.0.5",
    "typescript": "^3.9.5"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "jest": {
    "projects": [
      "<rootDir>/packages/*"
    ]
  }
}
